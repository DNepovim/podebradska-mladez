extends layouts/default

block content
  - if (have_posts()):
    - while (have_posts()) : the_post()
      .col-md-10.page.single
        h1=the_title()
        .text=the_content()
    - endwhile; else:
    .col-md-10.page.single
      h1='Error 404 - Not Found'
      .text
        p='Stránka, kterou hledáte, zde není...'
  - endif;
